<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login.touristReg">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="425dp"
        android:layout_height="92dp"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="-19dp"
        android:rotation="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vector_1" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="433dp"
        android:layout_height="91dp"
        android:layout_marginStart="-1dp"
        android:layout_marginBottom="-19dp"
        android:rotation="181"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/vector_1" />

    <ScrollView
        android:layout_width="413dp"
        android:layout_height="744dp"
        android:layout_marginTop="72dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:text="@string/register"
                android:textColor="@color/TextColor"
                android:textSize="@dimen/headings"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.469"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="84dp"
                android:text="@string/tourist"
                android:textColor="@color/black"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="100dp"
                android:text="@string/name"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <EditText
                android:id="@+id/TourRegName"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/name_hint"
                android:inputType="text"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView22" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:text="@string/age"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegName" />

            <EditText
                android:id="@+id/TourRegAge"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/age_hint"
                android:inputType="number"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView24" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:text="@string/country"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegAge" />

            <EditText
                android:id="@+id/TourRegCountry"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/country_hint"
                android:inputType="text"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView25" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:text="@string/pass_no"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegCountry" />

            <EditText
                android:id="@+id/TourRegPass"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/pass_hint"
                android:inputType="text"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView26" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:text="@string/contact_no"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegPass" />

            <EditText
                android:id="@+id/TourRegConact"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/contact_hint"
                android:inputType="phone"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView27" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:text="@string/email"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegConact" />

            <EditText
                android:id="@+id/TourRegEmail"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/sample_email"
                android:inputType="textEmailAddress"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView28" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:text="@string/password"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegEmail" />

            <EditText
                android:id="@+id/TourRegPassword"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/sample_password"
                android:inputType="textPassword"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView29" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:text="@string/confirm_pass"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegPassword" />

            <EditText
                android:id="@+id/TourRegConPaas"
                android:layout_width="@dimen/reg_form_width"
                android:layout_height="@dimen/reg_form_hight"
                android:layout_marginStart="64dp"
                android:layout_marginTop="24dp"
                android:background="@drawable/form_squre"
                android:ems="10"
                android:hint="@string/sample_password"
                android:inputType="textPassword"
                android:paddingStart="10dp"
                android:textSize="@dimen/regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView30" />

            <Button
                android:id="@+id/btnSignUp"
                android:layout_width="150dp"
                android:layout_height="55dp"
                android:layout_marginTop="52dp"
                android:background="@drawable/btn_yellow"
                android:text="@string/singup"
                android:textColor="@color/TextColor"
                android:textSize="@dimen/button_heading"
                app:circularflow_radiusInDP="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.517"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TourRegConPaas" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:text="@string/or"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnSignUp" />

            <TextView
                android:id="@+id/textView33"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:text="@string/have_account"
                android:textColor="@color/black"
                android:textSize="@dimen/regular"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView32" />

            <Button
                android:id="@+id/btnSignIn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:background="@drawable/btn_blue"
                android:backgroundTint="@color/TextColor"
                android:text="@string/signin"
                android:textColor="@color/btn_yellow"
                android:textSize="@dimen/regular"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView33" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>